<h1>New User</h1>

<%= form_for @user, controller: 'users', action: 'update' do |f| %>
Username: <%= f.text_field :username %>

Password: <%= f.password_field :password %>

Password Confirmation: <%= f.password_field :password_confirmation %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :city %>
<%= f.text_field :city %>

<%= f.label :state %>
<%= f.text_field :state %>

<%= f.label :bio %>
<%= f.text_field :bio %>



<%= f.submit "Submit" %>
<% end %>