<div id="overall-profile-container">
<div id="profile-header">
    <h1>My Profile</h1>
</div>

<div id="profile-container" class="card">
  <i id="profile-icon" class="far fa-user fa-7x"></i>
  <div class="card-body">
    <h5 class="card-text">Username: <%= current_user.username %></h5>
    <p class="class-text">City: <%= current_user.city %></p>
    <p class="class-text">State: <%= current_user.state %></p>
    <p class="class-text">Email: <%= current_user.email %></p>
    <p class="card-text">
        Bio: <%= current_user.bio %>
    </p>
  </div>
  <div id="profile-session-links" class="card-body">
    <%= link_to "Edit Profile", edit_user_path(current_user), class: "card-link btn btn-secondary" %>

    <%= link_to "Log Out",({controller: 'sessions', action: 'destroy', method: 'post'}), method: "POST", 
    class: "card-link btn btn-danger" %>
  </div>
</div>
</div>