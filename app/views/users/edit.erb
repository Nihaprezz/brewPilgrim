<h1>Edit Page for <%= current_user.username %></h1>

<p style="color: red;">Edit Not working Yet</p>

<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation, "Password Confirmation" %>
    <%= f.password_field :password_confirmation %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <%= f.submit "Submit" %>
<% end %>
 